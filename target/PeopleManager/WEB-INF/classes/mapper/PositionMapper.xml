<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cdp.dao.PositionDao">
    <insert id="add" parameterType="com.cdp.model.Position">
        insert into positioning(J_id,J_name,J_introduce,id,gz) values (#{J_id},#{J_name},#{J_introduce},#{id},#{gz})
    </insert>
</mapper>